        "passport": "^0.6.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.2.0",
        "uuid": "^9.0.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.1",
